@model IEnumerable<PrismaCatalogo.Front.Cliente.Models.FotoViewModel>

<div id="carouselFotos" class="carousel slide carousel-dark  slide" data-bs-ride="carousel">
    <div class="carousel-inner" style="height: 100%;">
        @foreach (var foto in Model)
        {
            <div class="carousel-item  @(foto.FgPrincipal == true ? "active" : "")" style="height: 100%;">
                @* <img class="rounded" src="data:image/png;base64,@Html.DisplayFor(modelItem => foto.FotoBytes)" class="d-block" alt="..."> *@
                <img class="rounded" src="@foto.Caminho" class="d-block" alt="...">
            </div>
        }
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselFotos" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselFotos" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>


<script>
    var meuCarrossel = new bootstrap.Carousel(document.getElementById("carouselFotos"), {
        interval: false // Impede movimento automático
    });
</script>